{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build",
    "build:prod": "run-s build:client build:prerender",
    "build:client": "ng build --prod --build-optimizer",
    "build:prerender": "node prerender.js",
    "test": "ng test",
    "test:client": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/es.json --clean --sort --format namespaced-json --marker _",
    "firebase:deploy": "firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.8",
    "@angular/common": "~7.2.8",
    "@angular/compiler": "~7.2.8",
    "@angular/core": "~7.2.8",
    "@angular/forms": "~7.2.8",
    "@angular/platform-browser": "~7.2.8",
    "@angular/platform-browser-dynamic": "~7.2.8",
    "@angular/router": "~7.2.8",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-google-charts": "^0.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "materialize-css": "~1.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "~7.2.8",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/materialize-css": "~1.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "puppeteer": "^1.12.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
