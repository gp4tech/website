{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --prod=false",
    "build:prod": "run-s build:client build:prerender",
    "build:client": "ng build --prod --build-optimizer",
    "build:prerender": "node prerender.js",
    "test:dev": "ng test --watch=false",
    "lint:dev": "ng lint",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/es.json --clean --sort --format namespaced-json --marker _",
    "firebase:deploy-dev": "firebase deploy -P develop --only hosting",
    "env-keys-config:create-keys": "cat ./src/environments/keys-config.template.ts > ./src/environments/keys-config.ts",
    "env-keys-config:override-keys": "echo $KEYS_CONFIG > ./src/environments/keys-config.ts",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular/animations": "^8.2.7",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/router": "^8.2.7",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-google-charts": "^0.1.6",
    "angulartics2": "^7.5.2",
    "core-js": "^2.6.9",
    "firebase": "^6.3.4",
    "materialize-css": "~1.0.0-rc.2",
    "rxjs": "^6.5.3",
    "ts-md5": "^1.2.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular/cli": "^8.3.5",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/language-service": "^8.2.7",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/materialize-css": "^1.0.6",
    "@types/node": "^8.9.5",
    "codelyzer": "^5.0.1",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^10.5.4",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "protractor": "^5.4.2",
    "puppeteer": "^1.20.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.5.3"
  },
  "lint-staged": {
    "*.{js,ts,scss,html,md}": "prettier --write"
  }
}
